<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suyashlakhotia.WhyBills" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.suyashlakhotia.WhyBills.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.suyashlakhotia.WhyBills.ExpenseActivity"
            android:label="@string/title_activity_expense"
            android:theme="@style/ListFont">
        </activity>
        <activity
            android:name="com.suyashlakhotia.WhyBills.ReceiptsView"
            android:label="@string/title_activity_receipts_view"
            android:theme="@style/ListFont">
        </activity>
    </application>

</manifest>
